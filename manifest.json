{
    "manifest_version": 2,
    "name": "Borderize",
    "description": "This extension adds a border to all elements without a data-id attribute.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Borderize!",
        "default_popup": "popup.html"
    }
}
